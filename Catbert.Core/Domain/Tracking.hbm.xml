<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="CAESDO.Catbert.Core.Domain.Tracking, CAESDO.Catbert.Core"
    table="Tracking" >
    <id name="id" access="field" column="TrackingID" unsaved-value="0">
      <generator class="identity" />
    </id>
    
    <property name="UserName" column="UserName" />
    <property name="ActionDate" column="ActionDate" />
    <property name="Comments" column="Comments" />
    
  </class>

  <class
    name="CAESDO.Catbert.Core.Domain.TrackingType, CAESDO.Catbert.Core"
    table="TrackingTypes" >
    <id name="id" access="field" column="TrackingTypeID" unsaved-value="0">
      <generator class="identity" />
    </id>

    <property name="Name" column="TrackingType" />

  </class>

  <class
    name="CAESDO.Catbert.Core.Domain.TrackingAction, CAESDO.Catbert.Core"
    table="TrackingActions" >
    <id name="id" access="field" column="TrackingActionID" unsaved-value="0">
      <generator class="identity" />
    </id>

    <property name="Name" column="TrackingAction" />

  </class>
</hibernate-mapping>