<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="CAESDO.Catbert.Core.Domain.User, CAESDO.Catbert.Core"
    table="Users" >
    <id name="id" access="field" column="UserID" unsaved-value="0">
      <generator class="identity" />
    </id>
    
    <property name="FirstName" column="FirstName" />
    <property name="LastName" column="LastName" />
    <property name="EmployeeID" column="EmployeeID" />
    <property name="StudentID" column="StudentID" />
    <property name="UserKey" column="UserKey"></property>
    <property name="Inactive" column="Inactive"></property>

    <bag name="Units" table="UserUnit" inverse="false">
      <key column="UserID" />
      <many-to-many column="UnitID" class="CAESDO.Catbert.Core.Domain.Unit, CAESDO.Catbert.Core" />
    </bag>

  </class>
</hibernate-mapping>