<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="CAESDO.Catbert.Core.Domain.User, CAESDO.Catbert.Core"
    table="Users" >
    <id name="ID" access="nosetter.lowercase" column="UserID" unsaved-value="0">
      <generator class="identity" />
    </id>
    
    <property name="FirstName" column="FirstName" />
    <property name="LastName" column="LastName" />

    <property name="LoginID" column="LoginID" />
    <property name="Phone" column="Phone" />
    <property name="Email" column="Email" />
    
    <property name="EmployeeID" column="EmployeeID" />
    <property name="StudentID" column="StudentID" />
    <property name="UserKey" column="UserKey"></property>
    <property name="Inactive" column="Inactive"></property>

    <bag name="UnitAssociations" table="UnitAssociations" inverse="false" cascade="none">
      <key column="UserID" />
      <many-to-many column="UnitAssociationID" class="CAESDO.Catbert.Core.Domain.UnitAssociation, CAESDO.Catbert.Core" />
    </bag>

    <bag name="Permissions" table="Permissions" inverse="false" cascade="none">
      <key column="UserID" />
      <many-to-many column="PermissionID" class="CAESDO.Catbert.Core.Domain.Permission, CAESDO.Catbert.Core" />
    </bag>
    
  </class>
</hibernate-mapping>