<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="CAESDO.Catbert.Core.Domain.UnitAssociation, CAESDO.Catbert.Core"
    table="UnitAssociations" >
    <id name="ID" access="nosetter.lowercase" column="UnitAssociationID" unsaved-value="0">
      <generator class="identity" />
    </id>
    
    <property name="Inactive" column="Inactive" />

    <many-to-one name="User" column="UserID"
      class="CAESDO.Catbert.Core.Domain.User, CAESDO.Catbert.Core" not-null="true" />

    <many-to-one name="Application" column="ApplicationID"
      class="CAESDO.Catbert.Core.Domain.Application, CAESDO.Catbert.Core" not-null="true" />

    <many-to-one name="Unit" column="UnitID"
      class="CAESDO.Catbert.Core.Domain.Unit, CAESDO.Catbert.Core" not-null="true" fetch="join" />
    
  </class>
</hibernate-mapping>