<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="CAESDO.Catbert.Core.Domain.Message, CAESDO.Catbert.Core"
    table="Messages" >
    <id name="ID" access="nosetter.lowercase" column="ID" unsaved-value="0">
      <generator class="identity" />
    </id>

    <property name="MessageText" column="Message" />
    <property name="BeginDisplayDate" column="BeginDisplayDate" />
    <property name="EndDisplayDate" column="EndDisplayDate" />
    <property name="IsActive" column="IsActive"></property>
    
    <many-to-one name="Application" class="CAESDO.Catbert.Core.Domain.Application, CAESDO.Catbert.Core" column="ApplicationID" cascade="none" />

  </class>
</hibernate-mapping>