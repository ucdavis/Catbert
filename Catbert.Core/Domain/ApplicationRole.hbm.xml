<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="CAESDO.Catbert.Core.Domain.ApplicationRole, CAESDO.Catbert.Core"
    table="ApplicationRoles" >
    <id name="ID" access="nosetter.lowercase" column="ApplicationRoleID" unsaved-value="0">
      <generator class="identity" />
    </id>
    
    <property name="Level" column="Level"></property>

    <many-to-one name="Application" column="ApplicationID" class="CAESDO.Catbert.Core.Domain.Application, CAESDO.Catbert.Core"></many-to-one>
    
    <many-to-one name="Role" column="RoleID" class="CAESDO.Catbert.Core.Domain.Role, CAESDO.Catbert.Core" fetch="join"></many-to-one>

  </class>
</hibernate-mapping>